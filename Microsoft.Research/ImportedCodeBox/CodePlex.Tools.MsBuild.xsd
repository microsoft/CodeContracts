        </xs:attribute>
        <xs:attribute name="OutputDirectory" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>Gets or sets the path to write the converted files</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Pdf" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>[Optional] Gets or sets a value indicating if the files have to be converted to pdf</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Xps" type="xs:boolean">
          <xs:annotation>
            <xs:documentation>[Optional] Gets or sets a value indicating if the files have to be converted to xps</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
